{
  "preset": "react-native",
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
  "testMatch": [
    "**/__tests__/**/*.test.js"
  ],
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!src/**/*.test.js",
    "!src/**/index.js"
  ],
  "coverageDirectory": "coverage",
  "coverageReporters": ["text", "lcov", "html"],
  "transformIgnorePatterns": [
    "node_modules/(?!(react-native|@react-native|react-native-vector-icons)/)"
  ]
}
